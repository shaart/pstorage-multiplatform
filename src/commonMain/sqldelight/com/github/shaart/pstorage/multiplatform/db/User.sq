createUser:
INSERT INTO usr_users(name, master_password, encrypt_type)
VALUES (:name, :masterPassword, :encryptionType);

findUserByName:
SELECT *
FROM usr_users
WHERE name = :name;

findUserByNameAndPassword:
SELECT *
FROM usr_users
WHERE name = :name AND master_password = :password;

findUserById:
SELECT *
FROM usr_users
WHERE id = :id;

lastInsertRowId:
SELECT last_insert_rowid();